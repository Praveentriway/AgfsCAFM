package com.daemon.emco_android.model.request;

import com.google.gson.annotations.SerializedName;

/**
 * Created by vikram on 8/3/18.
 */

public class PPMFindingRequest
{
    @SerializedName("companyCode")
    private String companyCode;

    public String getCompanyCode() {
        return companyCode;
    }

    public void setCompanyCode(String companyCode) {
        this.companyCode = companyCode;
    }

    public String getJobNo() {
        return jobNo;
    }

    public void setJobNo(String jobNo) {
        this.jobNo = jobNo;
    }

    public String getPpmNo() {
        return ppmNo;
    }

    public void setPpmNo(String ppmNo) {
        this.ppmNo = ppmNo;
    }

    public String getWorkOrderNo() {
        return workOrderNo;
    }

    public void setWorkOrderNo(String workOrderNo) {
        this.workOrderNo = workOrderNo;
    }

    @SerializedName("jobNo")
    private String jobNo;
    @SerializedName("ppmNo")
    private String ppmNo;
    @SerializedName("workOrderNo")
    private String workOrderNo;

    public PPMFindingRequest( String companyCode, String jobNo,String ppmNo,String workOrderNo) {
        this.companyCode = companyCode;
        this.jobNo = jobNo;
        this.ppmNo = ppmNo;
        this.workOrderNo = workOrderNo;
    }
}

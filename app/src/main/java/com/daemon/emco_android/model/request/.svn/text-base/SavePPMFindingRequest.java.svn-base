package com.daemon.emco_android.model.request;

import com.google.gson.annotations.SerializedName;

/**
 * Created by vikram on 8/3/18.
 */

public class SavePPMFindingRequest
{
    @SerializedName("companyCode")
    private String companyCode;
    @SerializedName("workOrderNo")
    private String workOrderNo;
    @SerializedName("jobNo")
    private String jobNo;
    @SerializedName("ppmNo")
    private String ppmNo;
    @SerializedName("assetCode")
    private String assetCode;
    @SerializedName("ppmFinding")
    private String ppmFinding;
    @SerializedName("ppmRecommend")
    private String ppmRecommend;
    @SerializedName("user")
    private String user;

    public String getCompanyCode() {
        return companyCode;
    }

    public void setCompanyCode(String companyCode) {
        this.companyCode = companyCode;
    }

    public String getWorkOrderNo() {
        return workOrderNo;
    }

    public void setWorkOrderNo(String workOrderNo) {
        this.workOrderNo = workOrderNo;
    }

    public String getJobNo() {
        return jobNo;
    }

    public void setJobNo(String jobNo) {
        this.jobNo = jobNo;
    }

    public String getPpmNo() {
        return ppmNo;
    }

    public void setPpmNo(String ppmNo) {
        this.ppmNo = ppmNo;
    }

    public String getAssetCode() {
        return assetCode;
    }

    public void setAssetCode(String assetCode) {
        this.assetCode = assetCode;
    }

    public String getPpmFinding() {
        return ppmFinding;
    }

    public void setPpmFinding(String ppmFinding) {
        this.ppmFinding = ppmFinding;
    }

    public String getPpmRecommend() {
        return ppmRecommend;
    }

    public void setPpmRecommend(String ppmRecommend) {
        this.ppmRecommend = ppmRecommend;
    }

    public String getUser() {
        return user;
    }

    public void setUser(String user) {
        this.user = user;
    }


    public SavePPMFindingRequest(String companyCode, String workOrderNo, String jobNo, String ppmNo, String assetCode, String ppmFinding, String ppmRecommend, String user) {
        this.companyCode = companyCode;
        this.workOrderNo = workOrderNo;
        this.jobNo = jobNo;
        this.ppmNo = ppmNo;

        this.assetCode = assetCode;
        this.ppmFinding = ppmFinding;
        this.ppmRecommend = ppmRecommend;
        this.user = user;
    }
}
